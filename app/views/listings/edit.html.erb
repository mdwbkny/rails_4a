<h1 id="head">Green Bay Real Estate</h1>
<div id="menu">
		<ul>
			<li><a href="/">Home</a></li>
			<li><a href="/listings">Listings</a></li>
		</ul>
	</div>

<center>
<h2>Edit Your Listing</h2>
<div id="form"><br>
<%= form_for @listing, url: {action: "update"} do |f| %>
	<%= f.label :address %>:<br>
	<%= f.text_field :address %><br><br>

	<%= f.label :unit %>:<br>
	<%= f.text_field :unit %><br><br>

	<%= f.label :price %>:<br> $
	<%= f.text_field :price %><br><br>

	<%= f.label :description %>:<br>
	<%= f.text_area :description %><br><br>

	<%= f.label :image %>:<br>
	<%= f.file_field :image %><br><br>

	<%= f.submit "Update" %>

<% end %>
</div>